<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%><html><head><meta http-equiv="Content-Type" content="text/html; charset=utf-8" /><title>喵姐早教说</title><%@include file="../header.jsp" %><link rel="stylesheet" href='<b:staticUrl>/res/css/mwebfeedback.css</b:staticUrl>'><style type="text/css">body{	overflow: auto;}</style><script type="text/javascript">$(function(){	$("#feedbackDiv").click(function(){		$("#feedbackForm").submit();	});	$("#replyBackDiv").click(function(){		$("#toReplyForm").submit();	});});</script></head><body><!-- <div class="header_btn" style="top: 0;position: fixed;width: 100%;z-index: 999; "> --><div class="header_btn">  <ul class="header_block">    <li><div id="feedbackDiv" class="border_r center">提交反馈</div></li>    <li class="current_a"><div  id="replyBackDiv"  class="current center">我的反馈</div></li>  </ul></div><!-- <div style="height:42px;width: 100%;"></div> --><div  style="display: none;">	<form id="toReplyForm" action="/yzmm/account/feedback/replyIndex" method="post">	</form>	<form id="feedbackForm" action="/yzmm/account/feedback/index" method="post">    </form></div><div id="loadingDiv" align="center" style="padding-top: 10px">数据加载中 <img alt="正在进行中" src="<b:staticUrl>/res/img/toolbar_load.gif</b:staticUrl>"/></div><div class="part1" id="wrapFirst"  style="display: none">	<table border="0" cellpadding="0" cellspacing="0">	    <tr>    		<td class="chat_btnA" valign="top">    			<div style="position: relative;width: 88px;min-height: 88px;">				<img src="<b:staticUrl>/res/img/photo_bg.png</b:staticUrl>"  class="wrapFirstImg" width="75px" height="75px" style="margin-left: 5px;margin-top: 5px;position: absolute;left: 0;top: 0">				<div  style="background: url(<b:staticUrl>/res/img/photo_bg_loop.png</b:staticUrl>);width: 88px;height: 88px;position: absolute; top: 0;left: 0"></div>    			</div>    		</td>        	<td class="chat_btnB">            <span class="letter_arrow"></span>            <div class="letter_text wrapFirstP"></div>            </td>        </tr>        <tr><td class="chat_btnA" align="center"><img src="<b:staticUrl>/res/img/dotted_bg3.png</b:staticUrl>" width="14" height="14"></td>            <td class="chat_btnA" align="center">            <span class="chat_time dlTxt_time" style="float:left"></span></td></tr>    </table><div class="clear"></div></div><div class="part1"  id="wrapSecond"  style="display: none">	<table border="0" cellpadding="0" cellspacing="0" align="right">	    <tr>    		<td class="chat_btnA">            	<span class="letter_arrowR"></span>            	<div class="letter_textA wrapSecondP"></div>            </td>        	<td class="chat_btnC"><img src="<b:staticUrl>/res/img/service_photo.png</b:staticUrl>" width="88" height="88" class="imgbtn">          </td>        </tr>        <tr><td class="chat_btnA" align="center"><span class="chat_time dlTxt_time" style="float:left"></span></td>            <td class="chat_btnA" align="center"><img src="<b:staticUrl>/res/img/dotted_bg4.png</b:staticUrl>" width="14" height="14">            </td></tr>    </table>    <div class="clear"></div></div><div id="wrap"></div><div class="clear" style="padding:10px;"></div>    <!--menu--><div id="submitDiv" style="bottom:0px; background-color:#e8e8e8; padding:10px;display: none;">	<table border="0" cellpadding="0" cellspacing="0" style="width: 100%">        <tr>        	<td align="center">        		<input id="contentIpt" name="contentIpt" type="text" style="padding:10px 0px; width:90%; color:#666666;font:18px/30px Microsoft YaHei,Arial, Helvetica, sans-serif;border: none;"   value="输入内容">&nbsp;&nbsp;        	</td>            <td width="70px" align="left">            	<button type="button" style="" class="button_sp_menu" onclick="submitParams()"><img src="<b:staticUrl>/res/img/button_sp_menu.png</b:staticUrl>" width="34" height="34" style="padding:0px 8px;"></button>            </td>         </tr>    </table></div><div class="clear" style="padding:10px;"></div><!--main end-->	<input type="hidden" id="domainKey" name="domainKey" value=""/>	<!--common content begin-->	<form  id="paramsForm" method="post">		<input type="hidden" id="main" name="main" value="false"/>		<input type="hidden" id="content" name="content"/>		<input type="hidden" id="groupId" name="groupId" value="${groupId}"/>		<input type="hidden" id="lastTime" name="lastTime"/>	</form>		<!--common content end-->	<script src="<b:staticUrl>/res/js/mwebFeedbackReply.js</b:staticUrl>"></script></body></html>